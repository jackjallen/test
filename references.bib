journal = {Medical Image Analysis},
keywords = {Brain morphometry,Medial shape description,Medical image analysis,Shape analysis,Voronoi skeleton},
number = {3},
pages = {207--220},
title = {{Statistical shape analysis of neuroanatomical structures based on medial models}},
