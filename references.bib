isbn = {1361-8415 (Print)
1361-8415 (Linking)},
journal = {Med Image Analysis},
keywords = {*Algorithms,*Artificial Intelligence,Heart/*anatomy \& histology,Humans,Image Enhancement/*methods,Image Interpretation, Computer-Assisted/*methods,Imaging, Three-Dimensional/*methods,Magnetic Resonance Imaging/*methods,Pattern Recognition, Automated/*methods,Reproducibility of Results,Sensitivity and Specificity,Software},
